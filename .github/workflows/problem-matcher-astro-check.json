{
  "problemMatcher": {
    "owner": "bennetrr/astro-check",
    "fileLocation": ["relative", "${workspaceFolder}"],
    "pattern": [
      {
        "regexp": "^(.*):(\\d+):(\\d+) - error ts\\(\\d+\\): (.*)$",
        "file": 1,
        "line": 2,
        "column": 3,
        "severity": "error",
        "message": 4
      }
    ]
  }
}
